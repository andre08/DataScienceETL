":"N","PRECISAO":0,"TAMANHO":255,"DESCRICAO":"Latitudo do endereço do Parceiro","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"LATITUDE","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":255,"DESCRICAO":"Longitude do Endereço do Parceiro","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"LONGITUDE","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":60,"DESCRICAO":"Cidade do Endereço do Parceiro","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"CIDADE","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":2,"DESCRICAO":"Estado do Endereço do Parceiro","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"ESTADO","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":40,"DESCRICAO":"Pais do Endereço da Pessoa","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"PAIS","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"}],"DESCRICAO":"","NOME":"PARCEIRO","CONEXAO":194975087},"SQL":"SELECT \n  IDPARCEIRO\n  , ORIGEM\n  , TIPPESSOA\n  , CODPARC\n  , NOME\n  , RAZAOSOCIAL\n  , CODIBGE\n  , LATITUDE\n  , LONGITUDE\n  , CIDADE\n  , ESTADO\n  , PAIS\nFROM\n\t(\n    SELECT\n      'SC-' || PAR.CODPARC AS IDPARCEIRO\n      , 'VENTISOL SC' AS ORIGEM\n      , Decode(CASE WHEN PAI.CODPAIS <> 32 THEN 'E' ELSE PAR.TIPPESSOA END, 'E', 'ESTRANGEIRO', 'F', 'FISICA', 'J', 'JURIDICA') AS TIPPESSOA\n      , PAR.CODPARC AS CODPARC\n      , PAR.NOMEPARC AS NOME\n      , PAR.RAZAOSOCIAL AS RAZAOSOCIAL\n      , CID.CODMUNSIAFI AS CODIBGE\n      , CID.LATITUDE\n      , CID.LONGITUDE\n      , CID.NOMECID AS CIDADE\n      , UFS.UF AS ESTADO\n      , UFS.DESCRICAO AS PAIS\n    FROM\n      SANKHYA.TGFPAR PAR\n      INNER JOIN SANKHYA.TSICID CID ON\n      CID.CODCID = PAR.CODCID\n      INNER JOIN SANKHYA.TSIUFS UFS ON\n      UFS.CODUF = CID.UF\n      INNER JOIN SANKHYA.TSIPAI PAI ON\n      PAI.CODPAIS = UFS.CODPAIS\n    WHERE\n      EXISTS (SELECT 0 FROM SANKHYA.TGFCAB CAB WHERE CAB.CODPARC = PAR.CODPARC)\n\n    UNION\n\n    SELECT\n      'NE-' || PAR.CODPARC\n      , 'VENTISOL NE' AS ORIGEM\n      , Decode(CASE WHEN PAI.CODPAIS <> 32 THEN 'E' ELSE PAR.TIPPESSOA END, 'E', 'ESTRANGEIRO', 'F', 'FISICA', 'J', 'JURIDICA') AS TIPPESSOA\n      , PAR.CODPARC AS CODPARC\n      , PAR.NOMEPARC AS NOME\n      , PAR.RAZAOSOCIAL AS RAZAOSOCIAL\n      , CID.CODMUNSIAFI AS CODIBGE\n      , CID.LATITUDE\n      , CID.LONGITUDE\n      , CID.NOMECID AS CIDADE\n      , UFS.UF AS ESTADO\n      , UFS.DESCRICAO AS PAIS\n    FROM\n      NORDESTE.TGFPAR PAR\n      INNER JOIN NORDESTE.TSICID CID ON\n      CID.CODCID = PAR.CODCID\n      INNER JOIN NORDESTE.TSIUFS UFS ON\n      UFS.CODUF = CID.UF\n      INNER JOIN NORDESTE.TSIPAI PAI ON\n      PAI.CODPAIS = UFS.CODPAIS\n    WHERE\n      EXISTS (SELECT 0 FROM NORDESTE.TGFCAB CAB WHERE CAB.CODPARC = PAR.CODPARC)\n\n    UNION\n\n    SELECT\n      'AM-' || PAR.CODPARC\n      , 'VENTISOL AM' AS ORIGEM\n      , Decode(CASE WHEN PAI.CODPAIS <> 32 THEN 'E' ELSE PAR.TIPPESSOA END, 'E', 'ESTRANGEIRO', 'F', 'FISICA', 'J', 'JURIDICA') AS TIPPESSOA\n      , PAR.CODPARC AS CODPARC\n      , PAR.NOMEPARC AS NOME\n      , PAR.RAZAOSOCIAL AS RAZAOSOCIAL\n      , CID.CODMUNSIAFI AS CODIBGE\n      , CID.LATITUDE\n      , CID.LONGITUDE\n      , CID.NOMECID AS CIDADE\n      , UFS.UF AS ESTADO\n      , UFS.DESCRICAO AS PAIS\n    FROM\n      MANAUS.TGFPAR PAR\n      INNER JOIN MANAUS.TSICID CID ON\n      CID.CODCID = PAR.CODCID\n      INNER JOIN MANAUS.TSIUFS UFS ON\n      UFS.CODUF = CID.UF\n      INNER JOIN MANAUS.TSIPAI PAI ON\n      PAI.CODPAIS = UFS.CODPAIS\n    WHERE\n      EXISTS (SELECT 0 FROM NORDESTE.TGFCAB CAB WHERE CAB.CODPARC = PAR.CODPARC)\n  )\nORDER BY IDPARCEIRO\n","CONEXAO":-1090300701},{"DES
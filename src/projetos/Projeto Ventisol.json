","TIPO":"DATE","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":7,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"DTVENC","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":50,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"HISTORICO","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"S","TIPO":"INT","OBRIGATORIO":"S","PRECISAO":0,"TAMANHO":3,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"DESDOBRAMENTO","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"NUMBER","OBRIGATORIO":"N","PRECISAO":-127,"TAMANHO":39,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"VLRDESDOB","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"NUMBER","OBRIGATORIO":"N","PRECISAO":-127,"TAMANHO":39,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"VLRMOVIMENTO","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"DATE","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":7,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"DHBAIXA","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"NUMBER","OBRIGATORIO":"N","PRECISAO":-127,"TAMANHO":39,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"VLRBAIXA","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":20,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"NOSSONUM","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"NUMBER","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":6,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"RECDESP","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"NUMBER","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":6,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"CODTIPTIT","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":50,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"DESCRTIPTIT","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"NUMBER","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":6,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"CODBCO","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":50,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"NOMEBCO","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"}],"DESCRICAO":"","NOME":"FINANCEIRO","CONEXAO":194975087},"SQL":"SELECT\n  IDFINANCEIRO\n  , IDPARCEIRO\n  , ORIGEM\n  , NUFIN\n  , NUMNOTA\n  , DTNEG\n  , DTVENC\n  , HISTORICO\n  , DESDOBRAMENTO\n  , VLRDESDOB\n  , VLRMOVIMENTO\n  , DHBAIXA\n  , VLRBAIXA\n  , NOSSONUM\n  , RECDESP\n  , CODTIPTIT\n  , DESCRTIPTIT\n  , CODBCO\n  , NOMEBCO\nFROM\n  (\n    SELECT DISTINCT\n      'SC-' || FIN.NUFIN AS IDFINANCEIRO\n      , 'SC-' || PAR.CODPARC AS IDPARCEIRO\n      , 'VENTISOL SC' AS ORIGEM\n      , FIN.NUFIN\n      , FIN.NUMNOTA\n      , FIN.DTNEG\n      , FIN.DTVENC\n      , SUBSTR(FIN.HISTORICO, 1, 50) AS HISTORICO\n      , FIN.DESDOBRAMENTO\n      , FIN.VLRDESDOB\n      , ((Nvl(FIN.VLRDESDOB, 0) + Nvl(FIN.DESPCART, 0) + Nvl(FIN.VLRJURO, 0) +  Nvl(FIN.VLRMULTA, 0)) - (NVL(FIN.VLRBAIXA, 0) + NVL(FIN.VLRDESC, 0) + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM SANKHYA.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) )) AS VLRMOVIMENTO\n      , FIN.DHBAIXA\n      , FIN.VLRBAIXA + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM SANKHYA.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) AS VLRBAIXA\n      , FIN.NOSSONUM\n      , FIN.RECDESP\n      , FIN.CODTIPTIT\n      , (SELECT DESCRTIPTIT FROM SANKHYA.TGFTIT WHERE CODTIPTIT = FIN.CODTIPTIT) AS DESCRTIPTIT\n      , FIN.CODBCO\n      , (SELECT NOMEBCO FROM SANKHYA.TSIBCO WHERE CODBCO = FIN.CODBCO) AS NOMEBCO \n    FROM\n      SANKHYA.TGFPAR PAR\n      INNER JOIN SANKHYA.TGFFIN FIN ON\n      FIN.CODPARC = PAR.CODPARC\n      INNER JOIN SANKHYA.TGFTOP TOP ON\n      TOP.CODTIPOPER = FIN.CODTIPOPER\n      AND TOP.DHALTER = FIN.DHTIPOPER\n    WHERE\n      FIN.VLRDESDOB >= 0\n      AND FIN.CODNAT <> 8060000\n      AND FIN.CODNAT NOT LIKE '6%'\n      AND FIN.CODEMP = 1\n      AND FIN.PROVISAO = 'N'\n      AND (TOP.ATUALFIN <> 0 OR TOP.CODTIPOPER IN (700, 707, 799))\n      AND FIN.RECDESP IN (1)\n      AND TOP.TIPMOV IN ('V', 'D', 'E', 'C', 'I')\n\n    UNION \n\n    SELECT DISTINCT \n      'NE-' || FIN.NUFIN AS IDFINANCEIRO\n      , 'NE-' || PAR.CODPARC AS IDPARCEIRO\n      , 'VENTISOL NE' AS ORIGEM\n      , FIN.NUFIN\n      , FIN.NUMNOTA\n      , FIN.DTNEG\n      , FIN.DTVENC\n      , SUBSTR(FIN.HISTORICO, 1, 50) AS HISTORICO\n      , FIN.DESDOBRAMENTO\n      , FIN.VLRDESDOB\n      , ((Nvl(FIN.VLRDESDOB, 0) + Nvl(FIN.DESPCART, 0) + Nvl(FIN.VLRJURO, 0) +  Nvl(FIN.VLRMULTA, 0)) - (NVL(FIN.VLRBAIXA, 0) + NVL(FIN.VLRDESC, 0) + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM NORDESTE.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) )) AS VLRMOVIMENTO\n      , FIN.DHBAIXA\n      , FIN.VLRBAIXA + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM NORDESTE.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) AS VLRBAIXA\n      , FIN.NOSSONUM\n      , FIN.RECDESP\n      , FIN.CODTIPTIT\n      , (SELECT DESCRTIPTIT FROM NORDESTE.TGFTIT WHERE CODTIPTIT = FIN.CODTIPTIT) AS DESCRTIPTIT\n      , FIN.CODBCO\n      , (SELECT NOMEBCO FROM NORDESTE.TSIBCO WHERE CODBCO = FIN.CODBCO) AS NOMEBCO \n    FROM\n      NORDESTE.TGFPAR PAR\n      INNER JOIN NORDESTE.TGFFIN FIN ON\n      FIN.CODPARC = PAR.CODPARC\n      INNER JOIN NORDESTE.TGFTOP TOP ON\n      TOP.CODTIPOPER = FIN.CODTIPOPER\n      AND TOP.DHALTER = FIN.DHTIPOPER\n    WHERE\n      FIN.VLRDESDOB >= 0\n      AND FIN.CODNAT <> 8060000\n      AND FIN.CODNAT NOT LIKE '6%'\n      AND FIN.CODEMP IN (1, 2, 3)\n      AND FIN.PROVISAO = 'N'\n      AND (TOP.ATUALFIN <> 0 OR TOP.CODTIPOPER IN (901))\n      AND FIN.RECDESP IN (1)\n      AND TOP.TIPMOV IN ('V', 'D', 'E', 'C', 'I')\n\n    UNION \n\n    SELECT DISTINCT \n      'AM-' || FIN.NUFIN AS IDFINANCEIRO\n      , 'AM-' || PAR.CODPARC AS IDPARCEIRO\n      , 'VENTISOL AM' AS ORIGEM\n      , FIN.NUFIN\n      , FIN.NUMNOTA\n      , FIN.DTNEG\n      , FIN.DTVENC\n      , SUBSTR(FIN.HISTORICO, 1, 50) AS HISTORICO\n      , FIN.DESDOBRAMENTO\n      , FIN.VLRDESDOB\n      , ((Nvl(FIN.VLRDESDOB, 0) + Nvl(FIN.DESPCART, 0) + Nvl(FIN.VLRJURO, 0) +  Nvl(FIN.VLRMULTA, 0)) - (NVL(FIN.VLRBAIXA, 0) + NVL(FIN.VLRDESC, 0) + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM MANAUS.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) )) AS VLRMOVIMENTO\n      , FIN.DHBAIXA\n      , FIN.VLRBAIXA + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM MANAUS.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) AS VLRBAIXA\n      , FIN.NOSSONUM\n      , FIN.RECDESP\n      , FIN.CODTIPTIT\n      , (SELECT DESCRTIPTIT FROM MANAUS.TGFTIT WHERE CODTIPTIT = FIN.CODTIPTIT) AS DESCRTIPTIT\n      , FIN.CODBCO\n      , (SELECT NOMEBCO FROM MANAUS.TSIBCO WHERE CODBCO = FIN.CODBCO) AS NOMEBCO \n    FROM\n      MANAUS.TGFPAR PAR\n      INNER JOIN MANAUS.TGFFIN FIN ON\n      FIN.CODPARC = PAR.CODPARC\n      INNER JOIN MANAUS.TGFTOP TOP ON\n      TOP.CODTIPOPER = FIN.CODTIPOPER\n      AND TOP.DHALTER = FIN.DHTIPOPER\n      LEFT JOIN DEPS.AD_REPRESENTANTE REP ON\n      REP.CODIGOSISTEMAGESTAO = FIN.CODVEND\n      AND REP.PROCESSADO = 'S'\n      AND REP.NOMEORIGEM = 'VENTISOL AM'\n    WHERE\n      FIN.VLRDESDOB >= 0\n      AND FIN.CODNAT <> 8060000\n      AND FIN.CODNAT NOT LIKE '6%'\n      AND FIN.CODEMP IN (1, 2)\n      AND FIN.PROVISAO = 'N'\n      AND (TOP.ATUALFIN <> 0 OR TOP.CODTIPOPER IN (901))\n      AND FIN.RECDESP IN (1)\n      AND TOP.TIPMOV IN ('V', 'D', 'E', 'C', 'I')\n  )\nORDER BY \n  IDFINANCEIRO\n  , DESDOBRAMENTO\n","CONEXAO":-1090300701},{"DESCRICAO":"Analise de pedidos feito no Deps","NOME":"Analise de Pedidos","ENTIDADE":{"ATRIBUTOS":[{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"varchar","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":50,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"Codigo","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"varchar","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":200,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"Razao","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"varchar","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":40,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"Origem","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"varchar","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":30,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"NPedido","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"decimal","OBRIGATORIO":"N","PRECISAO":2,"TAMANHO":20,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"Saldo","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"numeric","OBRIGATORIO":"N","PRECISAO":2,"TAMANHO":20,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"Valor","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"varchar","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":10,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"CondicaoPagamento","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"varchar","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":50,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"PrimiraAlteracao","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAV
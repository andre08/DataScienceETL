,"NOME":"LATITUDE","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":255,"DESCRICAO":"Longitude do Endereço do Parceiro","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"LONGITUDE","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":60,"DESCRICAO":"Cidade do Endereço do Parceiro","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"CIDADE","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":2,"DESCRICAO":"Estado do Endereço do Parceiro","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"ESTADO","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"},{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"N","TIPO":"VARCHAR","OBRIGATORIO":"N","PRECISAO":0,"TAMANHO":40,"DESCRICAO":"Pais do Endereço da Pessoa","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"PAIS","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"}],"DESCRICAO":"","NOME":"PARCEIRO","CONEXAO":194975087},"SQL":"SELECT\n  'SC-' || PAR.CODPARC AS IDPARCEIRO\n  , 'VENTISOL SC' AS ORIGEM\n  , Decode(CASE WHEN PAI.CODPAIS <> 32 THEN 'E' ELSE PAR.TIPPESSOA END, 'E', 'ESTRANGEIRO', 'F', 'FISICA', 'J', 'JURIDICA') AS TIPPESSOA\n  , PAR.CODPARC AS CODPARC\n  , PAR.NOMEPARC AS NOME\n  , PAR.RAZAOSOCIAL AS RAZAOSOCIAL\n  , CID.CODMUNSIAFI AS CODIBGE\n  , CID.LATITUDE\n  , CID.LONGITUDE\n  , CID.NOMECID AS CIDADE\n  , UFS.UF AS ESTADO\n  , UFS.DESCRICAO AS PAIS\nFROM\n  SANKHYA.TGFPAR PAR\n  INNER JOIN SANKHYA.TSICID CID ON\n  CID.CODCID = PAR.CODCID\n  INNER JOIN SANKHYA.TSIUFS UFS ON\n  UFS.CODUF = CID.UF\n  INNER JOIN SANKHYA.TSIPAI PAI ON\n  PAI.CODPAIS = UFS.CODPAIS\nWHERE\n  EXISTS (SELECT 0 FROM SANKHYA.TGFCAB CAB WHERE CAB.CODPARC = PAR.CODPARC)\n\nUNION\n\nSELECT\n  'NE-' || PAR.CODPARC\n  , 'VENTISOL NE' AS ORIGEM\n  , Decode(CASE WHEN PAI.CODPAIS <> 32 THEN 'E' ELSE PAR.TIPPESSOA END, 'E', 'ESTRANGEIRO', 'F', 'FISICA', 'J', 'JURIDICA') AS TIPPESSOA\n  , PAR.CODPARC AS CODPARC\n  , PAR.NOMEPARC AS NOME\n  , PAR.RAZAOSOCIAL AS RAZAOSOCIAL\n  , CID.CODMUNSIAFI AS CODIBGE\n  , CID.LATITUDE\n  , CID.LONGITUDE\n  , CID.NOMECID AS CIDADE\n  , UFS.UF AS ESTADO\n  , UFS.DESCRICAO AS PAIS\nFROM\n  NORDESTE.TGFPAR PAR\n  INNER JOIN NORDESTE.TSICID CID ON\n  CID.CODCID = PAR.CODCID\n  INNER JOIN NORDESTE.TSIUFS UFS ON\n  UFS.CODUF = CID.UF\n  INNER JOIN NORDESTE.TSIPAI PAI ON\n  PAI.CODPAIS = UFS.CODPAIS\nWHERE\n  EXISTS (SELECT 0 FROM NORDESTE.TGFCAB CAB WHERE CAB.CODPARC = PAR.CODPARC)\n\nUNION\n\nSELECT\n  'AM-' || PAR.CODPARC\n  , 'VENTISOL AM' AS ORIGEM\n  , Decode(CASE WHEN PAI.CODPAIS <> 32 THEN 'E' ELSE PAR.TIPPESSOA END, 'E', 'ESTRANGEIRO', 'F', 'FISICA', 'J', 'JURIDICA') AS TIPPESSOA\n  , PAR.CODPARC AS CODPARC\n  , PAR.NOMEPARC AS NOME\n  , PAR.RAZAOSOCIAL AS RAZAOSOCIAL\n  , CID.CODMUNSIAFI AS CODIBGE\n  , CID.LATITUDE\n  , CID.LONGITUDE\n  , CID.NOMECID AS CIDADE\n  , UFS.UF AS ESTADO\n  , UFS.DESCRICAO AS PAIS\nFROM\n  MANAUS.TGFPAR PAR\n  INNER JOIN MANAUS.TSICID CID ON\n  CID.CODCID = PAR.CODCID\n  INNER JOIN MANAUS.TSIUFS UFS ON\n  UFS.CODUF = CID.UF\n  INNER JOIN MANAUS.TSIPAI PAI ON\n  PAI.CODPAIS = UFS.CODPAIS\nWHERE\n  EXISTS (SELECT 0 FROM NORDESTE.TGFCAB CAB WHERE CAB.CODPARC = PAR.CODPARC)","CONEXAO":-1090300701},{"DESCRICAO":"Listando as origens","NOME":"Origem","ENTIDADE":{"ATRIBUTOS":[{"REFERENCIAATRIBUTO":0,"CHAVEPRIMARIA":"S","TIPO":"VARCHAR","OBRIGATORIO":"S","PRECISAO":0,"TAMANHO":11,"DESCRICAO":"","REFERENCIAENTIDADE":0,"OBSERVACAO":"","NOME":"NOME","VALORSEQUENCIAL":"N","CHAVEESTRANGEIRA":"N"}],"DESCRICAO":"","NOME":"ORIGEM","CONEXAO":194975087},"SQL":"SELECT 'VENTISOL SC' AS NOME FROM DUAL\nUNION SELECT 'VENTISOL NE' AS NOME FROM DUAL\nUNION SELECT 'VENTISOL AM' AS NOME FROM DUAL\nUNION SELECT 'BEMPLAS' AS NOME FROM DUAL","CONEXAO":-1090300701}],"MAPEAMENTO SA":[{"MAPEAMENTO ATRIBUTOS":[{"ATRIBUTO SA":15
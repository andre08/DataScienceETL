    <connection>SANKHYA</connection>
    <sql>SELECT
  IDFINANCEIRO
  , IDPARCEIRO
  , ORIGEM
  , NUFIN
  , NUMNOTA
  , DTNEG
  , DTVENC
  , HISTORICO
  , DESDOBRAMENTO
  , VLRDESDOB
  , VLRMOVIMENTO
  , DHBAIXA
  , VLRBAIXA
  , NOSSONUM
  , RECDESP
  , CODTIPTIT
  , DESCRTIPTIT
  , CODBCO
  , NOMEBCO
FROM
  (
    SELECT DISTINCT
      'SC-' || FIN.NUFIN AS IDFINANCEIRO
      , 'SC-' || PAR.CODPARC AS IDPARCEIRO
      , 'VENTISOL SC' AS ORIGEM
      , FIN.NUFIN
      , FIN.NUMNOTA
      , FIN.DTNEG
      , FIN.DTVENC
      , SUBSTR(FIN.HISTORICO, 1, 50) AS HISTORICO
      , FIN.DESDOBRAMENTO
      , FIN.VLRDESDOB
      , ((Nvl(FIN.VLRDESDOB, 0) + Nvl(FIN.DESPCART, 0) + Nvl(FIN.VLRJURO, 0) +  Nvl(FIN.VLRMULTA, 0)) - (NVL(FIN.VLRBAIXA, 0) + NVL(FIN.VLRDESC, 0) + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM SANKHYA.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) )) AS VLRMOVIMENTO
      , FIN.DHBAIXA
      , FIN.VLRBAIXA + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM SANKHYA.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) AS VLRBAIXA
      , FIN.NOSSONUM
      , FIN.RECDESP
      , FIN.CODTIPTIT
      , (SELECT DESCRTIPTIT FROM SANKHYA.TGFTIT WHERE CODTIPTIT = FIN.CODTIPTIT) AS DESCRTIPTIT
      , FIN.CODBCO
      , (SELECT NOMEBCO FROM SANKHYA.TSIBCO WHERE CODBCO = FIN.CODBCO) AS NOMEBCO 
    FROM
      SANKHYA.TGFPAR PAR
      INNER JOIN SANKHYA.TGFFIN FIN ON
      FIN.CODPARC = PAR.CODPARC
      INNER JOIN SANKHYA.TGFTOP TOP ON
      TOP.CODTIPOPER = FIN.CODTIPOPER
      AND TOP.DHALTER = FIN.DHTIPOPER
    WHERE
      FIN.VLRDESDOB >= 0
      AND FIN.CODNAT &lt;&gt; 8060000
      AND FIN.CODNAT NOT LIKE '6%'
      AND FIN.CODEMP = 1
      AND FIN.PROVISAO = 'N'
      AND (TOP.ATUALFIN &lt;&gt; 0 OR TOP.CODTIPOPER IN (700, 707, 799))
      AND FIN.RECDESP IN (1)
      AND TOP.TIPMOV IN ('V', 'D', 'E', 'C', 'I')

    UNION 

    SELECT DISTINCT 
      'NE-' || FIN.NUFIN AS IDFINANCEIRO
      , 'NE-' || PAR.CODPARC AS IDPARCEIRO
      , 'VENTISOL NE' AS ORIGEM
      , FIN.NUFIN
      , FIN.NUMNOTA
      , FIN.DTNEG
      , FIN.DTVENC
      , SUBSTR(FIN.HISTORICO, 1, 50) AS HISTORICO
      , FIN.DESDOBRAMENTO
      , FIN.VLRDESDOB
      , ((Nvl(FIN.VLRDESDOB, 0) + Nvl(FIN.DESPCART, 0) + Nvl(FIN.VLRJURO, 0) +  Nvl(FIN.VLRMULTA, 0)) - (NVL(FIN.VLRBAIXA, 0) + NVL(FIN.VLRDESC, 0) + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM NORDESTE.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) )) AS VLRMOVIMENTO
      , FIN.DHBAIXA
      , FIN.VLRBAIXA + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM NORDESTE.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) AS VLRBAIXA
      , FIN.NOSSONUM
      , FIN.RECDESP
      , FIN.CODTIPTIT
      , (SELECT DESCRTIPTIT FROM NORDESTE.TGFTIT WHERE CODTIPTIT = FIN.CODTIPTIT) AS DESCRTIPTIT
      , FIN.CODBCO
      , (SELECT NOMEBCO FROM NORDESTE.TSIBCO WHERE CODBCO = FIN.CODBCO) AS NOMEBCO 
    FROM
      NORDESTE.TGFPAR PAR
      INNER JOIN NORDESTE.TGFFIN FIN ON
      FIN.CODPARC = PAR.CODPARC
      INNER JOIN NORDESTE.TGFTOP TOP ON
      TOP.CODTIPOPER = FIN.CODTIPOPER
      AND TOP.DHALTER = FIN.DHTIPOPER
    WHERE
      FIN.VLRDESDOB >= 0
      AND FIN.CODNAT &lt;&gt; 8060000
      AND FIN.CODNAT NOT LIKE '6%'
      AND FIN.CODEMP IN (1, 2, 3)
      AND FIN.PROVISAO = 'N'
      AND (TOP.ATUALFIN &lt;&gt; 0 OR TOP.CODTIPOPER IN (901))
      AND FIN.RECDESP IN (1)
      AND TOP.TIPMOV IN ('V', 'D', 'E', 'C', 'I')

    UNION 

    SELECT DISTINCT 
      'AM-' || FIN.NUFIN AS IDFINANCEIRO
      , 'AM-' || PAR.CODPARC AS IDPARCEIRO
      , 'VENTISOL AM' AS ORIGEM
      , FIN.NUFIN
      , FIN.NUMNOTA
      , FIN.DTNEG
      , FIN.DTVENC
      , SUBSTR(FIN.HISTORICO, 1, 50) AS HISTORICO
      , FIN.DESDOBRAMENTO
      , FIN.VLRDESDOB
      , ((Nvl(FIN.VLRDESDOB, 0) + Nvl(FIN.DESPCART, 0) + Nvl(FIN.VLRJURO, 0) +  Nvl(FIN.VLRMULTA, 0)) - (NVL(FIN.VLRBAIXA, 0) + NVL(FIN.VLRDESC, 0) + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM MANAUS.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) )) AS VLRMOVIMENTO
      , FIN.DHBAIXA
      , FIN.VLRBAIXA + NVL((SELECT SUM(NVL(IMF.VALOR, 0)) FROM MANAUS.TGFIMF IMF WHERE IMF.NUFIN = FIN.NUFIN), 0) AS VLRBAIXA
      , FIN.NOSSONUM
      , FIN.RECDESP
      , FIN.CODTIPTIT
      , (SELECT DESCRTIPTIT FROM MANAUS.TGFTIT WHERE CODTIPTIT = FIN.CODTIPTIT) AS DESCRTIPTIT
      , FIN.CODBCO
      , (SELECT NOMEBCO FROM MANAUS.TSIBCO WHERE CODBCO = FIN.CODBCO) AS NOMEBCO 
    FROM
      MANAUS.TGFPAR PAR
      INNER JOIN MANAUS.TGFFIN FIN ON
      FIN.CODPARC = PAR.CODPARC
      INNER JOIN MANAUS.TGFTOP TOP ON
      TOP.CODTIPOPER = FIN.CODTIPOPER
      AND TOP.DHALTER = FIN.DHTIPOPER
      LEFT JOIN DEPS.AD_REPRESENTANTE REP ON
      REP.CODIGOSISTEMAGESTAO = FIN.CODVEND
      AND REP.PROCESSADO = 'S'
      AND REP.NOMEORIGEM = 'VENTISOL AM'
    WHERE
      FIN.VLRDESDOB >= 0
      AND FIN.CODNAT &lt;&gt; 8060000
      AND FIN.CODNAT NOT LIKE '6%'
      AND FIN.CODEMP IN (1, 2)
      AND FIN.PROVISAO = 'N'
      AND (TOP.ATUALFIN &lt;&gt; 0 OR TOP.CODTIPOPER IN (901))
      AND FIN.RECDESP IN (1)
      AND TOP.TIPMOV IN ('V', 'D', 'E', 'C', 'I')
  )
ORDER BY 
  IDFINANCEIRO
  , DESDOBRAMENTO
</sql>
